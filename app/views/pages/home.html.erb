<h1> User Models</h1>

<table>
    <tr>
        <td>name</td>
    </tr>
    <%@cats.each do |m| %>
        <tr>
            <td><%= m.name%></td>
        </tr>
    <%end%>
</table>

<table>
    <tr>
        <td>tasks</td>
        <td>finished</td>
    </tr>
    <%@todos.each do |t| %>
        <tr>
            <td><%= t.tasks%></td>
            <td><%= t.finished%></td>
        </tr>
    <%end%>
</table>

<table>
    <tr>
        <td>username</td>
        <td>email</td>
        <td>age</td>
    </tr>
    <%@users.each do |u| %>
        <tr>
            <td><%= u.username%></td>
            <td><%= u.email%></td>
            <td><%= u.age%></td>
        </tr>
    <%end%>
</table>
